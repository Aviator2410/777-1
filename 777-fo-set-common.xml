<?xml version="1.0"?>

<!--
    Usage: fgfs - -prop:/sim/remote/pilot-callsign="remote pilot callsign" - -aircraft=777-fo
-->

<PropertyList>

    <nasal>
        <b777>
            <file>Aircraft/777/Models/Instruments/ND/ND.nas</file>
            <!--file>Aircraft/777/Models/Instruments/MFD/mfd.nas</file>
            <file>Aircraft/777/Models/Instruments/MFD/fctl.nas</file>
            <file>Aircraft/777/Models/Instruments/MFD/eng.nas</file>
            <file>Aircraft/777/Models/Instruments/MFD/fuel.nas</file-->
        </b777>
        <dialogs>
            <file>Aircraft/777/Nasal/dialogs.nas</file>
        </dialogs>
        <!--cdu>
            <file>Aircraft/777/Models/Instruments/CDU/boeing.nas</file>
        </cdu-->
        <dual_control_tools>
            <file>Aircraft/Generic/DualControl/dual-control-tools.nas</file>
        </dual_control_tools>
        <aircraft_dual_control>
            <file>Aircraft/777/DualControl/777-dual-control.nas</file>
        </aircraft_dual_control>
        <dual_control>
            <file>Aircraft/Generic/DualControl/copilot-dual-control.nas</file>
        </dual_control>
    </nasal>
    <sim>
        <author>Hyde Yamakawa</author>
        <status>production</status>
        <rating>
            <FDM type="int">4</FDM>
            <systems type="int">4</systems>
            <cockpit type="int">4</cockpit>
            <model type="int">4</model>
        </rating>
        <aircraft-version>20140208</aircraft-version>
        <flight-model>null</flight-model>
        <model>
            <path>Aircraft/777/Models/777-fo.xml</path>
        </model>
        <sound>
            <path>Aircraft/777/Sounds/777-sound.xml</path>
        </sound>
        <airport>
            <runways>
                <start-offset-m>50</start-offset-m>
            </runways>
        </airport>
        <view n="0">
            <name>CoPilot View</name>
            <enabled type="bool">true</enabled>
            <type>lookfrom</type>
            <config n="0">
                <internal type="bool">true</internal>
                <from-model type="bool">true</from-model>
                <eye-lat-deg-path>/position/latitude-deg</eye-lat-deg-path>
                <eye-lon-deg-path>/position/longitude-deg</eye-lon-deg-path>
                <eye-alt-ft-path>/position/altitude-ft</eye-alt-ft-path>
                <eye-heading-deg-path>/orientation/heading-deg</eye-heading-deg-path>
                <eye-pitch-deg-path>/orientation/pitch-deg</eye-pitch-deg-path>
                <eye-roll-deg-path>/orientation/roll-deg</eye-roll-deg-path>
                <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
                <default-field-of-view-deg type="double">70.0</default-field-of-view-deg>
                <default-pitch-deg type="double">0</default-pitch-deg>
                <default-heading-deg type="double">0</default-heading-deg>
                <front-direction-deg type="double">0</front-direction-deg>
                <front-left-direction-deg type="double">45</front-left-direction-deg>
                <left-direction-deg type="double">90</left-direction-deg>
                <back-left-direction-deg type="double">135</back-left-direction-deg>
                <back-direction-deg type="double">180</back-direction-deg>
                <back-right-direction-deg type="double">225</back-right-direction-deg>
                <right-direction-deg type="double">270</right-direction-deg>
                <front-right-direction-deg type="double">315</front-right-direction-deg>
                <dynamic-view type="bool">true</dynamic-view>
                <x-offset-m type="double">0.60</x-offset-m>
                <y-offset-m type="double">0.8</y-offset-m>
                <pitch-offset-deg>-13.5</pitch-offset-deg>
                <limits>
                    <enabled type="bool">true</enabled>
                    <left>
                      <heading-max-deg type="double">140</heading-max-deg>
                      <x-offset-max-m type="double">0.45</x-offset-max-m>
                      <x-offset-threshold-deg type="double">45</x-offset-threshold-deg>
                    </left>
                    <right>
                        <heading-max-deg type="double">160</heading-max-deg>
                        <x-offset-max-m type="double">0.35</x-offset-max-m>
                        <x-offset-threshold-deg type="double">65</x-offset-threshold-deg>
                    </right>
                </limits>
            </config>
        </view>
        <view n="100">
            <name>Pilot View</name>
            <type>lookfrom</type>
            <config n="0">
                <internal type="bool">true</internal>
                <eye-lat-deg-path>/position/latitude-deg</eye-lat-deg-path>
                <eye-lon-deg-path>/position/longitude-deg</eye-lon-deg-path>
                <eye-alt-ft-path>/position/altitude-ft</eye-alt-ft-path>
                <eye-heading-deg-path>/orientation/heading-deg</eye-heading-deg-path>
                <eye-pitch-deg-path>/orientation/pitch-deg</eye-pitch-deg-path>
                <eye-roll-deg-path>/orientation/roll-deg</eye-roll-deg-path>
                <x-offset-m type="double">-0.57</x-offset-m>
                <y-offset-m type="double">0.8</y-offset-m>
                <default-field-of-view-deg type="double">70.0</default-field-of-view-deg>
                <pitch-offset-deg>-13.5</pitch-offset-deg>
                <limits>
                    <enabled type="bool">true</enabled>
                    <left>
                        <heading-max-deg type="double">140</heading-max-deg>
                        <x-offset-max-m type="double">0.35</x-offset-max-m>
                        <x-offset-threshold-deg type="double">65</x-offset-threshold-deg>
                    </left>
                    <right>
                        <heading-max-deg type="double">160</heading-max-deg>
                        <x-offset-max-m type="double">0.45</x-offset-max-m>
                        <x-offset-threshold-deg type="double">45</x-offset-threshold-deg>
                    </right>
                </limits>
            </config>
        </view>
        <chase-distance-m>-100</chase-distance-m>
        <yokes-visible type="bool">true</yokes-visible>
        <systems>
            <!--property-rule n="101">
                <path>Aircraft/777/Systems/777-PFD.xml</path>
            </property-rule-->
            <property-rule n="102">
                <path>Aircraft/777/Systems/777-pcu.xml</path>
            </property-rule>
        </systems>
        <instrumentation>
            <path>Aircraft/777/Systems/instrumentation.xml</path>
        </instrumentation>
    </sim>
    <controls>
        <engines>
            <engine n="0">
                <throttle-lever type="double">0</throttle-lever>
            </engine>
            <engine n="1">
                <throttle-lever type="double">0</throttle-lever>
            </engine>
        </engines>
        <flight>
            <speedbrake-lever type="int">0</speedbrake-lever>
        </flight>
    </controls>
    <instrumentation>
        <afds>
            <inputs>
                <indicated-vs-fpm type="double">0</indicated-vs-fpm>
                <AP type="bool">false</AP>
                <ias-mach-selected type="bool">false</ias-mach-selected>
                <reference-deg type="double">0</reference-deg>
            </inputs>
            <settings>
                <manual-intervention type="bool">false</manual-intervention>
            </settings>
            <!--ap-modes>
                <pitch-mode type="string">""</pitch-mode>
                <mode-annunciator type="string">""</mode-annunciator>
            </ap-modes-->
        </afds>
        <altimeter>
            <indicator-altitude-ft type="double">0</indicator-altitude-ft>
            <indicated-altitude-ft type="double">0</indicated-altitude-ft>
            <!--source-barometric type="string">"L"</source-barometric-->
            <setting-inhg type="double">29.92</setting-inhg>
        </altimeter>
        <airspeed-indicator>
            <indicator-speed-kt type="double">0</indicator-speed-kt>
        </airspeed-indicator>
        <efis>
            <inputs>
                <setting-std type="bool">false</setting-std>
            </inputs>
        </efis>
        <nav>
            <signal-quality-norm type="double">0</signal-quality-norm>
            <heading-needle-deflection-ptr type="double">0</heading-needle-deflection-ptr>
            <in-range type="double">0</in-range>
            <nav-loc type="double">0</nav-loc>
            <has-gs type="double">0</has-gs>
            <gs-in-range type="double">0</gs-in-range>
            <gs-needle-deflection-norm type="double">0</gs-needle-deflection-norm>
        </nav>
        <pfd>
            <runway-elevation-diff type="double">0</runway-elevation-diff>
            <target-altitude-diff type="double">0</target-altitude-diff>
            <minimum-diff type="double">0</minimum-diff>
            <target-speed-diff type="double">0</target-speed-diff>
            <stallspeed-diff type="double">0</stallspeed-diff>
            <overspeed-diff type="double">0</overspeed-diff>
            <speed-trend-up type="double">0</speed-trend-up>
            <speed-trend-down type="double">0</speed-trend-down>
            <vref-diff type="double">0</vref-diff>
            <vref-text-diff type="double">0</vref-text-diff>
            <v1-diff type="double">0</v1-diff>
            <vr-diff type="double">0</vr-diff>
            <v2-diff type="double">0</v2-diff>
            <flap-diff type="double">0</flap-diff>
            <fl1-diff type="double">0</fl1-diff>
            <fl5-diff type="double">0</fl5-diff>
            <fl15-diff type="double">0</fl15-diff>
        </pfd>
        <weu>
            <state>
                <vref type="double">0</vref>
                <stall-warning type="bool">false</stall-warning>
                <flap-on type="bool">false</flap-on>
                <fl1-on type="bool">false</fl1-on>
                <fl5-on type="bool">false</fl5-on>
                <fl15-on type="bool">false</fl15-on>
            </state>
        </weu>
    </instrumentation>
    <autopilot>
        <settings>
            <counter-set-altitude-ft type="int">10000</counter-set-altitude-ft>
            <counter-set-altitude-FL type="int">10000</counter-set-altitude-FL>
            <counter-set-altitude-100 type="int">10000</counter-set-altitude-100>
            <radio-altimeter-indication type="int">10000</radio-altimeter-indication>
            <target-speed-kt type="int">0</target-speed-kt>
            <heading-bug-deg type="int">360</heading-bug-deg>
            <transition-altitude type="int">18000</transition-altitude>
            <pfd-mach-indication type="int">0</pfd-mach-indication>
            <pfd-mach-target-indication type="int">0</pfd-mach-target-indication>
        </settings>
        <internal>
            <alt-alert type="int">0</alt-alert>
            <autopilot-transition type="int">0</autopilot-transition>
            <pitch-transition type="int">0</pitch-transition>
            <roll-transition type="int">0</roll-transition>
            <speed-transition type="int">0</speed-transition>
            <heading-bug-error-deg type="int">360</heading-bug-error-deg>
            <crab-angle-hdg type="double">0</crab-angle-hdg>
            <crab-angle-trk type="double">0</crab-angle-trk>
            <presision-loc type="int">0</presision-loc>
        </internal>
    </autopilot>
    <systems>
        <electrical>
            <outputs>
                <avionics type="bool">false</avionics>
                <efis type="bool">false</efis>
            </outputs>
            <CPT-FLT-INST type="double">0</CPT-FLT-INST>
        </electrical>
    </systems>
    <velocities>
        <groundspeed-kt type="double">0</groundspeed-kt>
    </velocities>
</PropertyList>
